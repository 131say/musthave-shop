{
  "name": "musthave",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "DATABASE_URL=\"file:./prisma/dev.db\" npx prisma generate && next build",
    "start": "next start -H 0.0.0.0",
    "lint": "eslint",
    "seed": "tsx scripts/seed.ts",
    "test:financial": "tsx scripts/testFinancialScenarios.ts",
    "reset-for-production": "node scripts/reset-for-production.mjs",
    "postinstall": "node -e \"process.env.DATABASE_URL ? require('child_process').execSync('npx prisma generate',{stdio:'inherit'}) : console.log('skip prisma generate: DATABASE_URL not set')\""
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/adapter-better-sqlite3": "^7.1.0",
    "@prisma/client": "^7.1.0",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^3.0.3",
    "better-sqlite3": "^12.5.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "next": "16.0.8",
    "prisma": "^7.1.0",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "recharts": "^3.7.0",
    "sharp": "^0.34.5",
    "sqlite3": "^5.1.7"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.0.8",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
